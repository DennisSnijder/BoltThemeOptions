{
    "name": "snijder/bolt-ui-options",
    "description": "Makes it easy to create an option UI for your Bolt site.",
    "type": "bolt-extension",
    "keywords": [
      "option",
      "options",
      "settings"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Snijder",
            "email": "Dennis@Snijder.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Snijder\\BoltUIOptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Snijder\\BoltUIOptions\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Snijder\\BoltUIOptions\\BoltUIOptionsExtension",
        "branch-alias": {
          "dev-master": "3.0.x-dev"
        },
        "bolt-icon": "assets/icon.png",
        "bolt-screenshots": [
            "assets/screenshots/main-options.png",
            "assets/screenshots/setting-yml.png"
        ]
    }
}
